<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>fo-dicom.Platform</id>
        <version>$version$</version>
        <title>Fellow Oak DICOM Platform Libraries</title>
        <authors>fo-dicom contributors</authors>
        <licenseUrl>http://opensource.org/licenses/MS-PL</licenseUrl>
        <projectUrl>https://github.com/fo-dicom/fo-dicom</projectUrl>
        <iconUrl>https://lh3.googleusercontent.com/-Fq3nigRUo7U/VfaIPuJMjfI/AAAAAAAAALo/7oaLrrTBhnw/s1600/Fellow%2BOak%2BSquare%2BTransp.png</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>.NET, UWP, Xamarin iOS and Xamarin Android specific support libraries for fo-dicom.</description>
        <language>en-US</language>
        <dependencies>
			<group targetFramework="net452">
				<dependency id="fo-dicom.Desktop" version="$version$" />
			</group>
			<group targetFramework="uap">
				<dependency id="fo-dicom.Universal" version="$version$" />
			</group>
			<group targetFramework="MonoAndroid10">
				<dependency id="fo-dicom.Android" version="$version$" />
			</group>
			<group targetFramework="Xamarin.iOS10">
				<dependency id="fo-dicom.iOS" version="$version$" />
			</group>
        </dependencies>
    </metadata>
    <files />
</package>