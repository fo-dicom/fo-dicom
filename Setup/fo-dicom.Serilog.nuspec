        <file src="..\Logging\Serilog.Desktop\bin\Release\Dicom.Serilog.xml" target="lib\net45" />
        <file src="..\Logging\Serilog\bin\Release\netstandard1.3\DICOM.Serilog.dll" target="lib\netstandard1.3" />
        <file src="..\Logging\Serilog\bin\Release\netstandard1.3\DICOM.Serilog.pdb" target="lib\netstandard1.3" />
    </files>
